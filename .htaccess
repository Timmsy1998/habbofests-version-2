# Various rewrite rules.
<IfModule mod_rewrite.c>
  RewriteEngine on
  RewriteCond %{REQUEST_FILENAME} !-f
  RewriteCond %{REQUEST_FILENAME} !-d
  RewriteCond %{REQUEST_URI} !=/favicon.ico
  RewriteRule ^([^/]+)$ index.php?p=$1 [L,QSA]
  #RewriteRule ^(.*)$ Z3us1b9Nx0/$1 [L,QSA]
